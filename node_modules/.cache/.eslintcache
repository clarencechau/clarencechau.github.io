=statistics&id=${videoId}&key=${apiKey}`\n      );\n      const data = await response.json();\n      const likesCount = data.items[0]?.statistics?.likeCount;\n      console.log(data);\n      setLikes(likesCount);\n    };\n\n    fetchLikes();\n  }, [videoId]);\n\n  return (\n    <div>\n      <p>\n        {likes ? (\n          <>\n            {likes}\n          </>\n        ) : (\n          \"Loading...\"\n        )}\n      </p>\n    </div>\n  );\n};\n\nexport default VideoLikes;\n",{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":1,"column":39,"nodeType":"91","messageId":"92","endLine":1,"endColumn":45},{"ruleId":"89","severity":1,"message":"93","line":108,"column":10,"nodeType":"91","messageId":"92","endLine":108,"endColumn":18},{"ruleId":"89","severity":1,"message":"94","line":109,"column":10,"nodeType":"91","messageId":"92","endLine":109,"endColumn":21},{"ruleId":"89","severity":1,"message":"95","line":109,"column":23,"nodeType":"91","messageId":"92","endLine":109,"endColumn":37},{"ruleId":"96","severity":1,"message":"97","line":286,"column":13,"nodeType":"98","messageId":"99","endLine":290,"endColumn":14,"fix":"100"},{"ruleId":"96","severity":1,"message":"97","line":293,"column":13,"nodeType":"98","messageId":"99","endLine":297,"endColumn":14,"fix":"101"},{"ruleId":"102","severity":1,"message":"103","line":19,"column":6,"nodeType":"104","endLine":19,"endColumn":15,"suggestions":"105"},{"ruleId":"89","severity":1,"message":"106","line":2,"column":8,"nodeType":"91","messageId":"92","endLine":2,"endColumn":15},{"ruleId":"102","severity":1,"message":"103","line":21,"column":6,"nodeType":"104","endLine":21,"endColumn":15,"suggestions":"107"},"no-new-object",["108"],"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","'isMobile' is assigned a value but never used.","'activeHover' is assigned a value but never used.","'setActiveHover' is assigned a value but never used.","react/jsx-no-target-blank","Using ta